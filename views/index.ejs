<h1>Skeleton Backend</h1>

<% if(user){ %>
<h2><%= user.name %></h2>
<p><%=user.email%></p>
<% }else{ %>
<p>No User Logged in.</p>
<%}%>

<div>
  <a href="/auth/login"><button>Login</button></a>
  <a href="/auth/signup"><button>Sign up</button></a>
  <a href="/auth/logout"><button>Logout</button></a>
</div>

<div>
  <a href="/example/public"><button>Public route</button></a>
  <a href="/example/private"><button>Private route</button></a>
</div>
